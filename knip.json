{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["scripts/**/*.{js,ts}"],
	"project": ["src/**/*.{ts,tsx}", "scripts/**/*.{js,ts}"],
	"ignore": [
		"dist/**",
		"coverage/**",
		"docs/**",
		"**/*.d.ts",
		"**/*.js.map",
		"**/*.d.ts.map",
		"tests/**/*.test.d.ts",
		"tests/**/*.test.js",
		"typedoc.json"
	],
	"ignoreDependencies": ["@types/inquirer", "ts-morph", "ts-node", "ultracite"],
	"ignoreBinaries": [
		"powershell",
		"docker-compose",
		"license-checker",
		"skott",
		"Apache-2.0",
		"BSD-2-Clause",
		"BSD-3-Clause",
		"ISC",
		"0BSD",
		"CC0-1.0",
		"Unlicense"
	],
	"ignoreUnresolved": [
		"../../src/core/schema-discovery",
		"../../src/core/validator",
		"../../src/utils/performance-monitor",
		"../../src/core/unified-config-system"
	],
	"ignoreExportsUsedInFile": true
}
